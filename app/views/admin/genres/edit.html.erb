<div class="container">
 <h5><span style="background-color:#DDDDDD">ジャンル編集</span></h5>
  <div class="low justify-content-center">
     <div class="container">
       <%= form_with model: @genre, url: admin_genre_path,method: :patch do |f| %>
          <%= f.label :name, "ジャンル名" %>
          <%= f.text_field :name %>
          <%= f.submit '変更を保存',class: 'btn-sm btn-success' %>
          <% end %>
     </div>
  </div>
</div>