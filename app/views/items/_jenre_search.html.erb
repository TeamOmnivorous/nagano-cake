<table class="genres_search">
  <thead>
    <tr>
      <th><h2>ジャンル検索</h2></th>
  </tr>
</thead>
<tbody>
    <% genres.each do |genre| %>
    <tr>
      <% if genres.is_active == true %>
      <td>
        <%= link_to genre.name, items_path(genre_id: genre.id), class: "text-dark" %><!--genre.name登録したジャンルが出力される？-->
      </td>                    <%# items_pathに引数としてgenre_idを渡す %>
      <% end %>
    </tr>
    <% end %>
</tbody>

